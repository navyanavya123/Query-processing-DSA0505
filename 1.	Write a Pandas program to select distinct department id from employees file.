import pandas as pd

# Assuming the data is stored in a DataFrame named 'employees'
employees = pd.read_csv("employees.csv")  # Replace "employees.csv" with the actual file path

# Select distinct department IDs
distinct_department_ids = employees['DEPARTMENT_ID'].unique()

# Display the distinct department IDs
print("Distinct Department IDs:")
for department_id in distinct_department_ids:
    print(department_id)
