import matplotlib.pyplot as plt

# Sample data
x = [1, 2, 3, 4, 5]
y1 = [2, 4, 6, 8, 10]
y2 = [1, 3, 5, 7, 9]

# Plot the lines with different colors and widths
plt.plot(x, y1, label='Line 1', color='blue', linewidth=2)
plt.plot(x, y2, label='Line 2', color='red', linewidth=3)

# Add legends
plt.legend()

# Add labels and title
plt.xlabel('X Axis')
plt.ylabel('Y Axis')
plt.title('Two Lines Plot with Legends')

# Show the plot
plt.grid(True)
plt.show()
